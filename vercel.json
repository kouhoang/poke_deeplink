{
    "buildCommand": "flutter/bin/flutter build web --release --base-href /",
    "outputDirectory": "build/web",
    "installCommand": "if cd flutter; then git pull && cd .. ; else git clone https://github.com/flutter/flutter.git; fi && ls && flutter/bin/flutter doctor && flutter/bin/flutter clean && flutter/bin/flutter config --enable-web",
    "framework": null,
    "routes": [
        {
            "src": "/.well-known/assetlinks.json",
            "dest": "/.well-known/assetlinks.json",
            "headers": {
                "Content-Type": "application/json"
            }
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/pokemon/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}